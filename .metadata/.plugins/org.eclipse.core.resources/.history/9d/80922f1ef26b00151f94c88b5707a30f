package KuramotoUnit;

public class UnitManage03 extends UnitManage {

	public UnitManage03(double K, double dt) {
		super(K, dt);
	}
	
	public Unit createUnit(double w) {
		Unit u = new Unit(w, dt, K);
		allunit.add(u);
		return u;
	}
	
	
	public void setUnit(Unit u, int index) {
		allunit.set(index, u);
	}

}
